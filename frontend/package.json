{
  "$schema": "https://json.schemastore.org/package",
  "name": "vacman-frontend",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0",
    "pnpm": ">=10.0"
  },
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm run build:application && pnpm run build:server",
    "build:application": "react-router build",
    "build:server": "vite build --config ./vite.server.config.ts",
    "dev": "nodemon --config ./nodemon.json",
    "preview": "pnpm run build && cross-env NODE_ENV=production SESSION_COOKIE_SECURE=false node --env-file-if-exists=./.env --import ./build/server/telemetry.js ./build/server/server.js",
    "start": "cross-env NODE_ENV=production node --import ./build/server/telemetry.js ./build/server/server.js",
    "test": "vitest",
    "test:e2e": "playwright test",
    "format:check": "prettier --cache --cache-location ./node_modules/.cache/prettier/ --check ./",
    "format:write": "prettier --cache --cache-location ./node_modules/.cache/prettier/ --write ./",
    "lint:check": "eslint --cache --cache-location: ./node_modules/.cache/eslint/ ./",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.206.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.206.0",
    "@opentelemetry/otlp-exporter-base": "^0.206.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/express": "^7.9.4",
    "@react-router/node": "^7.9.4",
    "@react-router/serve": "^7.9.4",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "connect-redis": "^8.1.0",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "i18next": "^25.5.3",
    "i18next-fetch-backend": "^6.0.0",
    "ioredis": "^5.8.1",
    "isbot": "^5.1.31",
    "jose": "^6.1.0",
    "libphonenumber-js": "^1.12.23",
    "minimatch": "^10.0.3",
    "morgan": "^1.10.1",
    "oauth4webapi": "^3.8.2",
    "oxide.ts": "^1.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.0.0",
    "react-idle-timer": "^5.7.2",
    "react-number-format": "^5.4.4",
    "react-phone-number-input": "^3.4.12",
    "react-router": "^7.9.4",
    "tailwind-merge": "^3.3.1",
    "valibot": "^1.1.0",
    "winston": "^3.18.3",
    "winston-error-format": "^3.0.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.56.0",
    "@react-router/dev": "^7.9.4",
    "@tailwindcss/vite": "^4.1.14",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/compression": "^1.8.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.18.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.9",
    "vite-plugin-static-copy": "^3.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
