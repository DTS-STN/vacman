{
  "$schema": "https://json.schemastore.org/package",
  "name": "vacman-support",
  "description": "Support software for the Vacancy Management solution.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">= 22.x",
    "pnpm": ">= 10.x"
  },
  "scripts": {
    "format:check": "prettier --cache --cache-location ./node_modules/.cache/prettier/ --check ./",
    "format:write": "prettier --cache --cache-location ./node_modules/.cache/prettier/ --write ./",
    "group-sync": "dotenv -- tsx ./src/group-sync.ts",
    "group-sync:debug": "dotenv -- tsx --inspect ./src/group-sync.ts",
    "lint:check": "eslint --cache --cache-location: ./node_modules/.cache/eslint/ ./",
    "lint:fix": "eslint --cache --cache-location: ./node_modules/.cache/eslint/ --fix ./",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@effect/cluster": "^0.55.0",
    "@effect/experimental": "^0.57.11",
    "@effect/platform": "^0.93.6",
    "@effect/platform-node": "^0.103.0",
    "@effect/rpc": "^0.72.2",
    "@effect/sql": "^0.48.6",
    "@effect/sql-mssql": "^0.49.0",
    "@effect/workflow": "^0.15.2",
    "effect": "^3.19.10"
  },
  "devDependencies": {
    "@effect/vitest": "^0.27.0",
    "@eslint/js": "^9.39.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.49.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
