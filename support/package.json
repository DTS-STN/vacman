{
  "$schema": "https://json.schemastore.org/package",
  "name": "vacman-support",
  "description": "Support software for the Vacancy Management solution.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">= 22.x",
    "pnpm": ">= 10.x"
  },
  "scripts": {
    "format:check": "prettier --cache --cache-location ./node_modules/.cache/prettier/ --check ./",
    "format:write": "prettier --cache --cache-location ./node_modules/.cache/prettier/ --write ./",
    "group-sync": "dotenv -- tsx ./src/group-sync.ts",
    "group-sync:debug": "dotenv -- tsx --inspect ./src/group-sync.ts",
    "lint:check": "eslint --cache --cache-location: ./node_modules/.cache/eslint/ ./",
    "lint:fix": "eslint --cache --cache-location: ./node_modules/.cache/eslint/ --fix ./",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@effect/platform": "^0.91.x",
    "@effect/platform-node": "^0.97.x",
    "effect": "^3.17.x"
  },
  "devDependencies": {
    "@effect/vitest": "^0.25.x",
    "@eslint/js": "^9.36.x",
    "@trivago/prettier-plugin-sort-imports": "^5.2.x",
    "@vitest/coverage-v8": "^3.2.x",
    "dotenv-cli": "^10.0.x",
    "eslint": "^9.36.x",
    "eslint-import-resolver-typescript": "^4.4.x",
    "eslint-plugin-import": "^2.32.x",
    "globals": "^16.4.x",
    "prettier": "^3.6.x",
    "tsx": "^4.20.x",
    "typescript-eslint": "^8.44.x",
    "vite-tsconfig-paths": "^5.1.x",
    "vitest": "^3.2.x",
    "vitest-mock-extended": "^3.1.x"
  }
}
